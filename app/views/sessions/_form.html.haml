= form_for :session, url: session_path do |form|
  .text-field
    = form.label :email
    = form.text_field :email, type: 'email'
  .password-field
    = form.label :password
    = form.password_field :password
  .submit-field
    = form.submit :class => "btn btn-primary"
  .other-links
    = link_to t(".forgot_password"), new_password_path
